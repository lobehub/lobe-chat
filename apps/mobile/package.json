{
  "name": "lobe-chat-react-native",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "expo prebuild",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "lint:fix": "eslint . --fix",
    "i18n": "npm run workflow:i18n && lobe-i18n && prettier -c --write \"locales/**\" && tsx ./scripts/i18nWorkflow/generateLocaleConfig.ts",
    "i18n:md": "lobe-i18n md",
    "workflow:i18n": "tsx ./scripts/i18nWorkflow/index.ts ",
    "prettier": "prettier -c --write \"**/**\"",
    "========================= local =======================": "",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "========================= device =======================": "",
    "device:ios": "expo run:ios --device",
    "device:android": "expo run:android --device",
    "device": "npm run device:ios & npm run device:android",
    "========================= preview =======================": "",
    "preview:android": "eas build -p android --profile preview",
    "preview:ios": "eas build -p ios --profile preview",
    "preview": "npm run preview:android & npm run preview:ios",
    "========================= preview:local =======================": "",
    "preview:android:local": "eas build -p android --profile preview --local",
    "preview:ios:local": "eas build -p ios --profile preview --local",
    "preview:local": "npm run preview:android & npm run preview:ios",
    "========================= production =======================": "",
    "production:android": "eas build --platform android--profile production",
    "production:ios": "eas build --platform ios --profile production",
    "production": "npm run build:android && npm run build:ios",
    "========================= production:local =======================": "",
    "production:ios:local": "eas build --platform ios --profile production --local",
    "production:android:local": "eas build --platform android --profile production --local",
    "production:local": "npm run build:android:local && npm run build:ios:local"
  },
  "dependencies": {
