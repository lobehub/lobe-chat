{
  "name": "lobe-chat-react-native",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android --port 8082",
    "build:playground": "npm run workflow:playground",
    "dev": "expo start",
    "device": "npm run device:ios & npm run device:android",
    "device:android": "expo run:android --device --port 8082",
    "device:ios": "expo run:ios --device  --port 8081",
    "eas-build-post-install": "rm -rf $TMPDIR/metro-cache",
    "eas-build-pre-install": "cd ../.. && NODE_OPTIONS='' pnpm install --ignore-scripts",
    "format": "prettier -c --write --no-error-on-unmatched-pattern \"**/**\"",
    "i18n": "npm run workflow:i18n && lobe-i18n && prettier -c --write \"locales/**\" && tsx ./scripts/i18nWorkflow/generateLocaleConfig.ts",
    "i18n:md": "lobe-i18n md",
    "ios": "expo run:ios --port 8081",
    "lint": "npm run type-check && npm run lint:ts && npm run lint:style && npm run lint:md",
    "lint:md": "remark . --quiet --frail --output",
    "lint:style": "stylelint \"{src,tests}/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:ts": "eslint \"{src,tests}/**/*.{js,jsx,ts,tsx}\" --fix",
    "prebuild": "expo prebuild",
    "preview": "npm run preview:android & npm run preview:ios",
    "preview:android": "eas build -p android --profile preview",
    "preview:android:local": "eas build -p android --profile preview --local",
    "preview:ios": "eas build -p ios --profile preview",
    "preview:ios:local": " NODE_OPTIONS='' eas build -p ios --profile preview --local",
    "preview:local": "npm run preview:android & npm run preview:ios",
    "production": "npm run production:android && npm run production:ios",
    "production:android": "eas build --platform android --profile production",
    "production:android:local": "NODE_OPTIONS='' eas build --platform android --profile production --local",
    "production:ios": "eas build --platform ios --profile production",
    "production:ios:local": "NODE_OPTIONS='' eas build --platform ios --profile production --local",
    "production:local": "npm run production:android:local && npm run production:ios:local",
    "reinstall": "cd ../.. && rm -rf node_modules apps/**/node_modules packages/**/node_modules && NODE_OPTIONS='' pnpm install && cd apps/mobile && rm -rf .expo ios android node_modules && pnpm install",
    "release:android": "eas build --platform android --profile production --non-interactive --auto-submit",
    "release:ios": "eas build --platform ios --profile production --non-interactive --auto-submit",
    "start": "expo start",
    "submit": "npm run submit:ios & npm run submit:android",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "test": "jest --coverage",
    "type-check": "tsx ./scripts/type-check.ts",
    "web": "expo start --web",
    "workflow:i18n": "tsx ./scripts/i18nWorkflow/index.ts ",
    "workflow:playground": "tsx ./scripts/playground/index.ts"
  },
