{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/**/*.ts{x,}", "next.config.mjs", "vitest.config.ts"],
  "ignore": [
    "**/__tests__/**",
    "**/*.test.ts{x,}",
